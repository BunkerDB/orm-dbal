{
    "name": "cratia/orm-dbal",
    "description": "Database abstraction layer (DBAL) for execute Query based on https://www.doctrine-project.org/projects/dbal.html",
    "type": "library",
    "require": {
        "cratia/orm-dql": "^0.1.0",
        "cratia/pipeline": "^0.5.0",
        "doctrine/dbal": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/dotenv": "^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Ratia",
            "email": "cratia.github@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Cratia\\ORM\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "test/"
        ],
        "psr-4": {
            "Tests\\Cratia\\ORM\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit"
    }
}
