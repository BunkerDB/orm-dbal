{
    "name": "bunkerdb/orm-dbal",
    "description": "Database abstraction layer (DBAL) for execute Query based on https://www.doctrine-project.org/projects/dbal.html",
    "type": "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bunkerdb/orm-dql"
        }
    ],
    "require": {
        "bunkerdb/orm-dql": "dev-master",
        "cratia/pipeline": ">=0.7",
        "doctrine/dbal": "^2.10",
        "php": ">=7.1",
        "ext-json": "*",
        "psr/log": "^1.1",
        "cratia/interface-adapter": ">=0.1",
        "cratia/common-functions": ">=0.1",
        "cratia/adapter-sql-performance": ">=0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/dotenv": "^5.0",
        "monolog/monolog": "^2.0",
        "cratia/adapter-mysql": ">=0.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Ratia",
            "email": "cratia.github@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Cratia\\ORM\\DBAL\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
            "Tests\\Cratia\\ORM\\DBAL\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit"
    }
}
