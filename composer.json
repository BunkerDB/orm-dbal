{
    "name": "bunkerdb/orm-dbal",
    "description": "Database abstraction layer (DBAL) for execute Query based on https://www.doctrine-project.org/projects/dbal.html",
    "type": "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bunkerdb/orm-dql"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bunkerdb/adapter-mysql"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bunkerdb/adapter-sql-performance"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bunkerdb/interface-adapter"
        }
    ],
    "require": {
        "bunkerdb/orm-dql": "dev-master",
        "cratia/pipeline": ">=0.7",
        "doctrine/dbal": "^4.0.2",
        "php": ">=7.1",
        "ext-json": "*",
        "psr/log": "^3.0",
        "bunkerdb/interface-adapter": "dev-master",
        "cratia/common-functions": ">=0.1",
        "bunkerdb/adapter-sql-performance": "dev-master",
        "bunkerdb/adapter-mysql": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/dotenv": "^5.0",
        "monolog/monolog": "^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Ratia",
            "email": "cratia.github@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Cratia\\ORM\\DBAL\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
            "Tests\\Cratia\\ORM\\DBAL\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable"
}
